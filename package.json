{
  "name": "@antv/g2",
  "version": "5.0.0",
  "description": "the Grammar of Graphics in Javascript",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "browser": "dist/g2.min.js",
  "types": "lib/index.d.ts",
  "files": [
    "src",
    "lib",
    "esm",
    "dist"
  ],
  "scripts": {
    "start": "genji-notebook dev",
    "deploy": "genji-notebook deploy",
    "clean": "rimraf lib esm dist",
    "lint-staged": "lint-staged",
    "size": "limit-size",
    "lint": "eslint ./src ./__tests__ && prettier ./src ./__tests__ --check ",
    "fix": "eslint ./src ./__tests__ --fix && prettier ./src ./__tests__ --write ",
    "test": "jest --coverage",
    "test-live": "cross-env DEBUG_MODE=1 jest --watch",
    "build:umd": "rimraf ./dist && rollup -c && npm run size",
    "build:cjs": "rimraf ./lib && tsc --module commonjs --outDir lib",
    "build:esm": "rimraf ./esm && tsc --module ESNext --outDir esm",
    "build": "run-p build:*",
    "bundle-vis": "cross-env BUNDLE_VIS=1 run-p build:umd",
    "ci": "run-s lint test build",
    "prepublishOnly": "npm run ci",
    "prepare": "husky install"
  },
  "keywords": [
    "antv",
    "g2",
    "visualization",
    "chart",
    "grammar",
    "graphics",
    "interaction",
    "animation"
  ],
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@types/d3-array": "^3.0.2",
    "@types/d3-shape": "^3.0.2",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.22.0",
    "eslint-plugin-import": "^2.22.1",
    "genji-notebook": "^0.0.10",
    "husky": "^7.0.0",
    "jest": "^26.0.1",
    "jest-electron": "^0.1.12",
    "limit-size": "^0.1.4",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rollup": "^2.39.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-polyfill-node": "^0.8.0",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-uglify": "^6.0.4",
    "rollup-plugin-visualizer": "^5.6.0",
    "ts-jest": "^26.5.1",
    "typescript": "^4.1.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "limit-size": [
    {
      "path": "dist/g2.min.js",
      "limit": "750 Kb",
      "gzip": true
    },
    {
      "path": "dist/g2.min.js",
      "limit": "2 Mb"
    }
  ],
  "author": {
    "name": "AntV",
    "url": "https://antv.vision/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/antvis/g2"
  },
  "bugs": {
    "url": "https://github.com/antvis/g2/issues"
  },
  "dependencies": {
    "@antv/coord": "^0.4.1",
    "@antv/g": "^5.0.20",
    "@antv/g-canvas": "^1.0.13",
    "@antv/gui": "^0.4.3-alpha.5",
    "@antv/scale": "^0.4.7",
    "@antv/util": "^2.0.17",
    "d3-array": "^3.1.1",
    "d3-shape": "^3.1.0",
    "d3-voronoi": "^1.1.4"
  }
}
